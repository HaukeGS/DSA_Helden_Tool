<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="paneLanguages" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.LanguageController">
	<children>
		<ListView fx:id="allLanguages" prefHeight="200.0" prefWidth="200.0" />
		<VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" HBox.hgrow="NEVER">
			<children>
				<Button fx:id="btnAssignLanguage" disable="true" mnemonicParsing="false" onAction="#assignLanguage" text="-&gt;" />
				<Button fx:id="btnUnAssignLanguage" disable="true" mnemonicParsing="false" onAction="#unassignLanguage" text="&lt;-" />
			</children>
		</VBox>
		<ListView fx:id="assignedLanguages" prefHeight="200.0" prefWidth="200.0" />
	</children>
</HBox>
